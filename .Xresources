URxvt*font: xft:TamzenForPowerline:pixelsize=16
!URxvt*font: -*-tamzenforpowerline-medium-r-normal--14-*-*-*-*-*-*-*
!URxvt*font: -gohu-gohufont-medium-r-normal--14-100-100-100-c-80-iso10646-1
!URxvt*font: xft:Dina:pixelsize=13 
!URxvt*font: -xos4-terminesspowerline-medium-r-normal--14-140-72-72-c-80-iso10646-1

URxvt*scrollBar: false
URxvt*internalBorder: 5 

Xft.dpi: 96
Xft.antialias: true      
Xft.hinting: true
Xft.rgba: rgb 
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! special
*.foreground:   #c5c5c5
*.background:   #111111
*.cursorColor:  #aaaaaa

! black
*.color0:       #000000
*.color8:       #505050

! red
*.color1:       #ac4142
*.color9:       #ac4142

! green
*.color2:       #90a959
*.color10:      #90a959

! yellow
*.color3:       #f4bf75
*.color11:      #f4bf75

! blue
*.color4:       #6a9fb5
*.color12:      #6a9fb5

! magenta
*.color5:       #aa759f
*.color13:      #aa759f

! cyan
*.color6:       #75b5aa
*.color14:      #75b5aa

! white
*.color7:       #d0d0d0
*.color15:      #f5f5f5

! rofi
!                  background     border   separator
rofi.color-window: argb:f3000000, #000000, #1d1f21
! state:           bg             fg       bgalt          hlbg          hlfg
rofi.color-normal: argb:00000000, #f3f4f5, argb:00000000, #ac4142,      #000000
rofi.color-urgent: argb:00000000, #f3f4f5, argb:00000000, #ac4142,      #000000
rofi.color-active: argb:00ff0000, #f3f4f5, argb:00000000, #ac4142,      #000000

rofi.bw: 1

rofi.font: Tamzen 14px
rofi.eh: 1
rofi.padding: 2
rofi.width: 40
rofi.lines: 5
rofi.columns: 4
rofi.hide-scrollbar: true
rofi.separator-style: none
